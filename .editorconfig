tab_width=4

[{*.jhm,*.cscfg,*.jrxml,*.rng,*.wadcfgx,*.axml,*.wsdl,*.webref,*.fxml,*.myapp,*.svcmap,*.xslt,*.ant,*.xul,*.xsl,*.xsd,*.jnlp,*.csdef,*.disco,*.tld,*.xml}]
